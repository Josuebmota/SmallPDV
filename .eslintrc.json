{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": ["prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "use": true
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
      "prettier/prettier":"error",
      "class-methods-use-this": "off",
      "no-param-reassign": "off",
      "camelcase": "off",
      "no-unused-vars": ["error",{"argsIgnorePattern": "next"}]
    }
}
